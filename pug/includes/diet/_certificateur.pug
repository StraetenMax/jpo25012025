-
    const logosList = [
        {
            'image':'https://i.ibb.co/ctL8M04/Qualiopi.png',
            'alt':'logo Qualiopi, actions de formation et actions de formation en apprentissage'
        },
        {
            'image':'https://i.ibb.co/hy7kBKM/Plan-de-travail-1.png',
            'alt':'logo France Compétences certification enregistrée au RNCP'
        },
        {
            'image':'https://i.ibb.co/NmWvQ57/logo-Academie-Lillejpo12142024.png',
            'alt':'logo de l\'Académie de Lille'
        }
    ]
mj-section(background-color="#ffffff" padding="20px")
    mj-column
        mj-text(align="center" mj-class="T+4 cT+1") Nos Certifications et certificateurs
        mj-divider(align="center" border-color="#14141f" border-style="solid" border-width="0.75px" width="160px")
mj-section(background-color="#ffffff" padding-top="0" padding-bottom="20px" padding-left="60px" padding-right="60px") 
    mj-group
        each logo in logosList
            mj-column(width="33.333333%" padding="10px" vertical-align="middle")
                mj-image(padding="0" border="none" width="150px" src=logo.image alt=logo.alt)/