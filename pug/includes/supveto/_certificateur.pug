mj-section(background-color="#ffffff" padding-top="20px" padding-bottom="0px" padding-right="20px" padding-left="20px")
    mj-column
        mj-text(align="center" mj-class="T+4 cT+1") Nos certifications et certificateurs
        mj-divider(align="center" border-color="#14141f" border-width="0.75px" border-style="solid" width="160px")
mj-section(background-color="#ffffff" padding-top="0" padding-bottom="20px" padding-right="20px" padding-left="20px") 
    mj-group(width="50%")
        each logo in logosList1
            mj-column(width="50%" padding="10px" vertical-align="middle")
                mj-image(padding="0" border="none" width="150px" src=logo.image alt=logo.alt)/ 
    mj-group(width="50%")
        each logo in logosList2
            mj-column(width="50%" padding="10px" vertical-align="middle")
